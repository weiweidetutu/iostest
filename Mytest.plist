<plist version="1.0">
<dict>
    <key>items</key>
    <array>
        <dict>
            <key>assets</key>
            <array>
                <dict>
                    <key>kind</key>
                    <string>software-package</string>
                    <key>url</key>
                    <!-- 需要安装的ipa包在服务器的路径 -->
                    <string>https://192.168.10.200/ipa/mytest/Mytest.ipa</string>
                </dict>
                <dict>
                    <key>kind</key>
                    <string>full-size-image</string>
                    <key>needs-shine</key>
                    <true/>
                    <key>url</key>
                    <!-- 安装时需要显示的大图标（1024*1024）在服务器的路径 -->
                    <string>https://192.168.10.200/ipa/mytest/icon_full_size_s.png
                    </string>
                </dict>
                <dict>
                    <key>kind</key>
                    <string>display-image</string>
                    <key>needs-shine</key>
                    <true/>
                    <key>url</key>
                    <!-- 安装时需要显示的小图标（180*180）在服务器的路径 -->
                    <string>https://192.168.10.200/ipa/mytest/icon_display_s.png</string>
                </dict>
            </array>
            <key>metadata</key>
            <dict>
                <key>bundle-identifier</key>
                <string>com.Datathk.lnc.sdp</string>
                <key>bundle-version</key>
                <string>1.0.0</string>
                <key>kind</key>
                <string>software</string>
                <key>title</key>
                <string>SDP测试客户端</string>
            </dict>
        </dict>
    </array>
</dict>
</plist>
